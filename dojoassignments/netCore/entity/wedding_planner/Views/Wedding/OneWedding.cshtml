<h2>This is the page for one wedding!</h2>

<a asp-controller="Wedding" asp-action="Dashboard" class='btn btn-default'>Dashboard</a>

<div class='col-md-6'>
<h3>@ViewBag.Wedding.SpouseOne & @ViewBag.Wedding.SpouseTwo 's Wedding</h3>
<h4>Date: @ViewBag.Wedding.Date</h4>
<h4>Address: @ViewBag.Wedding.Address</h4>

<h5>Guests: Need to do</h5>

@foreach (var guest in @ViewBag.Wedding.Attendees)
{
    <p>@guest.User.FirstName @guest.User.LastName</p>
}
</div>
<div class='col-md-6'>
<iframe
  width="600"
  height="450"
  frameborder="0" style="border:0"
  src="https://www.google.com/maps/embed/v1/search?key=AIzaSyCTKKwG8jXtvQ_p1SCx6fP2ST0ypFXead4
    &q=@ViewBag.Wedding.Address.Replace(" ","+")" allowfullscreen>
</iframe>
</div>
